<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Style x:Key="CustomFontTextBlock" TargetType="TextBlock">
        <Setter Property="TextElement.FontFamily" Value="fonts/#Actor"/>
    </Style>
        
    <Style x:Key="CustomFontContentButton" TargetType="Button">
        <Setter Property="FontFamily" Value="fonts/#Actor"/>
    </Style>
        
    <Style x:Key="CustomFontTextBox" TargetType="TextBox">
        <Setter Property="FontFamily" Value="fonts/#Actor"/>
    </Style>
        
    <SolidColorBrush x:Key="CustomGray" Color="#C6C6C6"></SolidColorBrush>
    <SolidColorBrush x:Key="CustomPink" Color="#F96060"></SolidColorBrush>
</ResourceDictionary>